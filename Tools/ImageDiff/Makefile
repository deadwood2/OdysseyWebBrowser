SCRIPTS_PATH = ../Scripts

ifneq (,$(findstring iphone,$(SDKROOT)))
	OVERRIDE_SDKROOT = default
	OVERRIDE_ARCHS = default
endif
ifneq (,$(findstring watch,$(SDKROOT)))
	OVERRIDE_SDKROOT = default
	OVERRIDE_ARCHS = default
endif
ifneq (,$(findstring tv,$(SDKROOT)))
	OVERRIDE_SDKROOT = default
	OVERRIDE_ARCHS = default
endif

include ../../Makefile.shared
