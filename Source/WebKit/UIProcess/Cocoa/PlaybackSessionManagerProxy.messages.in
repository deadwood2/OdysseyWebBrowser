    LegibleMediaSelectionOptionsChanged(uint64_t contextId, Vector<WebCore::MediaSelectionOption> options, uint64_t selectedIndex)
    AudioMediaSelectionIndexChanged(uint64_t contextId, uint64_t selectedIndex)
